{
  "name": "@avada/console",
  "version": "0.2.1",
  "description": "Avada App CLI to save time for developers",
  "main": "src/index.ts",
  "bin": {
    "avada": "bin/avada.js"
  },
  "scripts": {
    "prepublish": "npm run lint && npm run build",
    "build": "npx crlf --set=LF node_modules/.bin/jira-prepare-commit-msg && tsc -d --p tsconfig.build.json",
    "watch": "npm run build -- --watch",
    "lint": "tslint src/**/.ts",
    "eslint-fix": "./node_modules/.bin/eslint --fix ./storage/template"
  },
  "dependencies": {
    "commander": "^3.0.2",
    "firebase-admin": "^9.9.0",
    "fs-extra": "^8.1.0",
    "http-proxy": "^1.18.0",
    "js-yaml": "^3.13.1",
    "ora": "^4.0.2",
    "string-template": "^1.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/http-proxy": "^1.17.0",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.11.1",
    "@types/ora": "^3.2.0",
    "@types/string-template": "^1.0.2",
    "lerna": "^3.16.4",
    "prettier": "^1.18.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.2",
    "eslint": "^6.3.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.16.0",
    "jira-prepare-commit-msg": "^1.6.2"
  },
  "gitHead": "7c207bac6eaeb76fae96690d72b24505a72c34f5"
}
